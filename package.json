{
    "name": "campus-connect",
    "private": true,
    "sideEffects": false,
    "type": "module",
    "scripts": {
        "build": "remix vite:build",
        "dev": "remix vite:dev",
        "serve": "remix-serve ./build/server/index.js",
        "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
        "start": "remix-serve ./build/server/index.js",
        "typecheck": "tsc",
        "migrate": "tsx ./db/src/scripts/migrate.ts && npm run types",
        "migrate:down": "tsx ./db/src/scripts/migrate.ts --down && npm run types",
        "migration:create": "tsx ./db/src/scripts/create-migration.ts",
        "seed": "tsx ./db/src/scripts/seed.ts && npm run types",
        "type-check": "tsc --noEmit",
        "types": "kysely-codegen --dialect=postgres --camel-case --out-file=./db/src/dist/db.d.ts"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.689.0",
        "@aws-sdk/lib-storage": "^3.689.0",
        "@radix-ui/themes": "^3.1.6",
        "@remix-run/node": "^2.14.0",
        "@remix-run/react": "^2.14.0",
        "@remix-run/serve": "^2.14.0",
        "@tailwindcss/container-queries": "^0.1.1",
        "googleapis": "^144.0.0",
        "isbot": "^4.1.0",
        "kysely": "^0.27.4",
        "pg": "^8.13.1",
        "prettier-plugin-tailwindcss": "^0.6.8",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-feather": "^2.0.10",
        "ts-pattern": "^5.5.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@remix-run/dev": "^2.14.0",
        "@types/pg": "^8.11.10",
        "@types/prompt-sync": "^4.2.3",
        "@types/react": "^18.2.20",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.7.4",
        "@typescript-eslint/parser": "^6.7.4",
        "autoprefixer": "^10.4.19",
        "commander": "^12.1.0",
        "dedent": "^1.5.3",
        "eslint": "^8.38.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "kysely-codegen": "^0.17.0",
        "postcss": "^8.4.38",
        "prompt-sync": "^4.2.0",
        "tailwind-merge": "^2.5.4",
        "tailwindcss": "^3.4.4",
        "tsx": "^4.19.2",
        "typescript": "^5.1.6",
        "vite": "^5.1.0",
        "vite-tsconfig-paths": "^4.2.1"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}
